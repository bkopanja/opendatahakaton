<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hakaton.tim.ejnabavke">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <meta-data android:name="AA_DB_NAME" android:value="ejnabavke.db" />
    <meta-data android:name="AA_DB_VERSION" android:value="1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"


    android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.IntroActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.FilterActivity"
            android:label="@string/activityFiltriranje"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.NotificationActivity"
            android:label="@string/activityNotifications"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.FeedActivity"
            android:label="@string/activityNotifications"
            android:screenOrientation="portrait"/>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
